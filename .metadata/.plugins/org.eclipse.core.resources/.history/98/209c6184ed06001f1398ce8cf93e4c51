package basic;
class Employees{
	private int id;
	private String name,address;
	private double salary;
	private long contact;
	public Employees(int id,String name,String address,double salary,long contact) {
		this.id = id;
		this.name = name;
		this.address = address;
		this.salary = salary;
		this.contact = contact;
	}
}
